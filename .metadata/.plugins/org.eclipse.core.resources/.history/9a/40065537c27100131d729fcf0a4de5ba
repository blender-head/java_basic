package org.blenderhead.chapter7;

//Use an array to pass a variable number of 
//arguments to a method.
public class PassArray {

	public static void main(String[] args) {

		System.out.print("Number of args: " + v.length + 
                " Contents: "); 

for(int x : v) 
System.out.print(x + " "); 

System.out.println(); 

	}

}
